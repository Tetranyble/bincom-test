<!doctype html>
<html lang="en">
  <%- include('./partials/header'); %>
  <body>
    <%- include('./partials/navigation'); %>
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="content">
            <h1 ><%= title %></h1>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                      <tr>
                        <th scope="col">Polling Unit</th>
                        <th scope="col">Party Abbreviation</th>
                        <th scope="col">Party Score</th>
                      </tr>
                    </thead>
                    <tbody>
                        <% result.forEach( p => {%>
                            <tr>
                              <td><%= p.polling_unit_uniqueid %></td>
                              <td><%= p.party_abbreviation %></td>
                              <td><%= p.party_score %></td>
                            </tr>
                         <% }); %>
                    </tbody>
                  </table>
            </div>
          </div>
        </div>
      </div>
    </div>
    <%- include('./partials/footer'); %>
  </body>
</html>
