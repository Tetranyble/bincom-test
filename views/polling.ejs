<!doctype html>
<html lang="en">
  <%- include('./partials/header'); %>
  <body>
    <%- include('./partials/navigation'); %>
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="content">
            <h1><%= title %></h1>
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">L.G.A Name</th>
                    <th scope="col">Party Abbreviation</th>
                    <th scope="col">Party Score</th>
                    <th scope="col">Entered By</th>
                    <th scope="col">Date Entered</th>
                    <th scope="col">IP Address</th>
                  </tr>
                </thead>
                <tbody>
                  
                    <tr>
                        <td><%= result.result_id %></td>
                        <td><%= result.lga_name %></td>
                        <td><%= result.party_abbreviation %></td>
                        <td><%= result.party_score %></td>
                        <td><%= result.entered_by_user %></td>
                        <td><%= result.date_entered %></td>
                        <td><%= result.user_ip_address %></td>
                    </tr>
                </tbody>
              </table>
          </div>
        </div>
      </div>
    </div>
    <%- include('./partials/footer'); %>
  </body>
</html>
